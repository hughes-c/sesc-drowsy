for i in `ls -1`
do
  report.pl $i > $$.$i.tmp 
  cat $$.$i.tmp | grep ALUIssueX | cut -f 10 -d " " > ../sescresults/`echo $i | sed "s/^\([^.]*\)[.].*/\1.seqInst/"`
  cat $$.$i.tmp | grep -E "^[ ][ ][ ]*[0-9]*[ ][ ][0-9.][0-9.]*" | grep -v "%" | grep -v "MHz" | sed "s/[ ][ ]*/ /g" | cut -f 5 -d ' ' > ../sescresults/`echo $i | sed "s/^\([^.]*\)[.].*/\1.seqCyc/"`
  rm $$.$i.tmp
done
